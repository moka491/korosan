{
  "name": "korosan",
  "version": "0.2.0",
  "engines": {
    "node": ">=14.0.0"
  },
  "description": "A discord bot using Discord.js and written in Typescript",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon",
    "build": "rimraf ./dist && tsc",
    "start": "npm run build && node dist/app.js",
    "test": "npm run build && jest",
    "test:coverage": "npm run build && jest --collectCoverage",
    "test:clearCache": "jest --clearCache",
    "test:uploadCoverageWindows": "codecov.exe -f coverage/coverage-final.json -t"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "keywords": [
    "discord.js",
    "discord",
    "bot",
    "typescript"
  ],
  "author": "Jannis Becker",
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.3.2",
    "@iarna/toml": "^2.2.5",
    "@types/bluebird": "^3.5.32",
    "@types/validator": "^13.1.0",
    "bufferutil": "^4.0.1",
    "chevrotain": "^7.0.1",
    "cron": "^1.8.2",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "erlpack": "github:discordapp/erlpack",
    "libsodium-wrappers": "^0.7.6",
    "lodash.merge": "^4.6.2",
    "pg": "^8.3.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.22.3",
    "sequelize-typescript": "^1.1.0",
    "utf-8-validate": "^5.0.2",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.23",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "mock-fs": "^4.12.0",
    "node-gyp": "^7.0.0",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.0.0",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  }
}
